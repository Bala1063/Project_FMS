<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
PRP_FMS:
@author Aruna M.
-->
<hibernate-mapping>
    <class name="ProjectFMS.Bean.AnswerBean" table="Answer_tb">
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
           <property name="questionId" type="string" column="Question_id"/>
        <property name="answerType" type="string" column="Answer_type"/>
        <property name="answer" type="string" column="Answer"/>
  
    </class>
</hibernate-mapping>

