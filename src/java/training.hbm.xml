<hibernate-mapping>
    <class name="projectfms.bean.TrainingBean" table="Training_tb">
        <id name="trainingId" type="string" column="Training_id">
            <generator class="assigned"/>
        </id>
        <property name="trainingName" type="string" column="Training_name"/>
        <list name="contentBean" cascade="all" lazy="false">
            <key column="Training_id"/>
            <index column="type"/>
            <one-to-many class="ProjectFMS.Bean.ContentBean"/>
        </list>
    </class>
</hibernate-mapping>


