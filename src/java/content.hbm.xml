<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
PRP_FMS:
@author Aruna M.
-->
<hibernate-mapping>
    <class name="ProjectFMS.Bean.ContentBean" table="Content_tb">
        <id name="contentId" type="string" column="Content_id">
            <generator class="ProjectFMS.IDgenerator.ContentIDgenerator"/>
        </id>
        <property name="contentName" type="string" column="Content_name"/>
        <property name="contentDate" type="date" column="Content_date"/>
        <property name="trainingId" type="string" column="Training_id"/>
       <property name="contentDescription" type="string" column="Content_description"/>
        </class>
</hibernate-mapping>



