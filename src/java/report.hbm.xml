<hibernate-mapping>
    <class name="ProjectFMS.Bean.ReportBean" table="Report_tb">
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="reportDate" type="date" column="Report_date"/>
        <property name="institution" type="string" column="Institution"/>
        <property name="batchId" type="string" column="Batch_id"/>
        <property name="totalStrength" type="int" column="Total_strength"/>
        <property name="attendedCandidates" type="int" column="Attended_candidates"/>
        <property name="percentile" type="float" column="Percentile"/>
   
    </class>
</hibernate-mapping>



